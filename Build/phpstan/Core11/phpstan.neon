includes:
  - ../../../.Build/vendor/saschaegerer/phpstan-typo3/extension.neon
  - phpstan-baseline.neon

parameters:
  level: 8
  reportUnmatchedIgnoredErrors: true
  tmpDir: ../../../.cache/phpstan

  paths:
    - ../../../Classes
    - ../../../Tests
    - ../../../Configuration
    - ../../../Migrations

  excludePaths:
    - ../../../.Build/*